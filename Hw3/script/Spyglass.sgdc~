#modify clk period & edge{0 half clk period}, do not modify clk2
current_design "top"
clock -name "top.clk" -domain clk -tag clk -period 11 -edge {0 5.5}
clock -name "top.clk2" -domain clk2 -tag clk2 -period 100 -edge {0 50}
reset -name "top.rst" -value 1 -sync
reset -name "top.rst2" -value 1 -sync

abstract_port -module SRAM -ports A0 A1 A2 A3 A4 A5 A6 A7 A8 A9 A10 A11 A12 A13 DO0 DO1 DO2 DO3 DO4 DO5 DO6 DO7 DO8 DO9 DO10 DO11 DO12 DO13 DO14 DO15 DO16 DO17 DO18 DO19 DO20 DO21 DO22 DO23 DO24 DO25 DO26 DO27 DO28 DO29 DO30 DO31 DI0 DI1 DI2 DI3 DI4 DI5 DI6 DI7 DI8 DI9 DI10 DI11 DI12 DI13 DI14 DI15 DI16 DI17 DI18 DI19 DI20 DI21 DI22 DI23 DI24 DI25 DI26 DI27 DI28 DI29 DI30 DI31 WEB0 WEB1 WEB2 WEB3 OE CS -clock clk

#reset CDC flip flops with initial value 0
#s1
define_tag -tag initState -name top.WDT_wrapper.w1.s1.d_out -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s1.r2w_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s1.r2w_ff_1 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s1.w2r_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s1.w2r_ff_1 -value 0

#s2
define_tag -tag initState -name top.WDT_wrapper.w1.s2.d_out -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s2.r2w_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s2.r2w_ff_1 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s2.w2r_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s2.w2r_ff_1 -value 0

#s3
define_tag -tag initState -name top.WDT_wrapper.w1.s3.d_out -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s3.r2w_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s3.r2w_ff_1 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s3.w2r_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s3.w2r_ff_1 -value 0

#s4
define_tag -tag initState -name top.WDT_wrapper.w1.s4.d_out -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s4.r2w_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s4.r2w_ff_1 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s4.w2r_ff_0 -value 0
define_tag -tag initState -name top.WDT_wrapper.w1.s4.w2r_ff_1 -value 0


#if there are any fifos, set fifo memory
#WDEN
fifo -memory "top.WDT_wrapper.w1.s1.d_out"

#WDLIVE
fifo -memory "top.WDT_wrapper.w1.s2.d_out"

#WTOCNT
fifo -memory "top.WDT_wrapper.w1.s3.d_out"

#WTO
fifo -memory "top.WDT_wrapper.w1.s4.d_out"


waive -rule Ar_syncrst_setupcheck01
